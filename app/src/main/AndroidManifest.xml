<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mezons.matrixapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/matrix_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/matrix_icon_circle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TwoMatrixValue" />
        <activity android:name=".Two_Matrix" />
        <activity android:name=".Single_Matrix" />
        <activity android:name=".MatrixVaue" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>